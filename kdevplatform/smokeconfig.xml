<config>
    <moduleName>kdevplatform</moduleName>
    <parentModules>
        <module>kde</module>
    </parentModules>
    <parts>10</parts>
    <scalarTypes>
        <!-- QString is a class, but represented as a scalar (#) in munged names -->
        <typeName>QString</typeName>
    </scalarTypes>
    <voidpTypes>
        <!-- both are classes, but they are represented as Smoke::t_voidp -->
        <typeName>QStringList</typeName>
        <typeName>QString</typeName>
    </voidpTypes>
    <exclude>
        <!-- private stuff -->
        <signature>.*::d</signature>
        <signature>.*::d_ptr</signature>
        <signature>.*::q_ptr</signature>
        <signature>.*::q</signature>
        <signature>.*QDebug.*</signature>
        <signature>.*Q3.*</signature>
        
        <signature>.*KDevelop::ItemRepositoryRegistry::ItemRepositoryRegistry\(const KDevelop::ItemRepositoryRegistry&amp;\)</signature>;
        <signature>.*KDevelop::ParsingEnvironmentFile::ParsingEnvironmentFile\(const KDevelop::ParsingEnvironmentFile&amp;\)</signature>
        
        <signature>.*KDevelop::ClassFunctionDeclaration::setIsExternalDefinition\(bool\)</signature>
        <signature>.*KDevelop::DUContext::scopeIdentifierInternal\(KDevelop::DUContext\*\) const</signature>
        <signature>.*KDevelop::EditorIntegrator::documentLoaded\(KTextEditor::Document\*\)</signature>
        <signature>.*KDevelop::EditorIntegrator::initialise\(\)</signature>
        <signature>.*KDevelop::EditorIntegrator::createCursor\(const KDevelop::LockedSmartInterface&amp;, std::size_t, KDevelop::EditorIntegrator::Edge\)</signature>
        <signature>.*KDevelop::DocumentRangeObject::setUrl\(const KDevelop::HashedString&amp;\)</signature>
        <signature>.*KDevelop::DocumentRangeObject::url\(\) const</signature>
        <signature>.*KDevelop::DocumentRangeObject::setRangeOwning\(KDevelop::DocumentRangeObject::RangeOwning\)</signature>
        <signature>.*KDevelop::DocumentRangeObject::ownsRange\(\) const</signature>
        <signature>.*KDevelop::DocumentController::documentType\(Sublime::Document\*\) const</signature>
        <signature>.*KDevelop::IProjectFileManager::fileRenamed\(const KUrl&amp;, KDevelop::ProjectFileItem\*\)</signature>
        <signature>.*KDevelop::IProjectFileManager::fileRemoved\(KDevelop::ProjectFileItem\*\)</signature>
        <signature>.*KDevelop::IProjectFileManager::fileAdded\(KDevelop::ProjectFileItem\*\)</signature>
        <signature>.*KDevelop::IProjectFileManager::folderRenamed\(const KUrl&amp;, KDevelop::ProjectFolderItem\*\)</signature>
        <signature>.*KDevelop::IProjectFileManager::folderRemoved\(KDevelop::ProjectFolderItem\*\)</signature>
        <signature>.*KDevelop::IProjectFileManager::folderAdded\(KDevelop::ProjectFolderItem\*\)</signature>
        <signature>.*KDevelop::IProjectFileManager::projectItemConfigWidget\(const QList&lt; KDevelop::ProjectBaseItem\* &gt;&amp;, KDevelop::KDialogBase\*\)</signature>
        <signature>.*KDevelop::IOutputView::toolViewRemoved\(int\)</signature>
        <signature>.*KDevelop::IOutputView::outputRemoved\(int, int\)</signature>
        <signature>.*KDevelop::IProjectBuilder::built\(KDevelop::ProjectBaseItem\*\)</signature>
        <signature>.*KDevelop::IProjectBuilder::installed\(KDevelop::ProjectBaseItem\*\)</signature>
        <signature>.*KDevelop::IProjectBuilder::cleaned\(KDevelop::ProjectBaseItem\*\)</signature>
        <signature>.*KDevelop::IProjectBuilder::failed\(KDevelop::ProjectBaseItem\*\)</signature>
        <signature>.*KDevelop::IProjectBuilder::configured\(KDevelop::IProject\*\)</signature>
        <signature>.*KDevelop::IProjectBuilder::pruned\(KDevelop::IProject\*\)</signature>
        <signature>.*KDevelop::UiController::defaultArea\(\)</signature>
        <signature>.*KDevelop::TypeRepository::indexForType\(TypePtr&lt; KDevelop::AbstractType &gt;\)</signature>
        <signature>.*KDevelop::TypeRepository::typeForIndex\(unsigned int\)</signature>
        <signature>.*KDevelop::TypeRepository::increaseReferenceCount\(unsigned int\)</signature>
        <signature>.*KDevelop::TypeRepository::decreaseReferenceCount\(unsigned int\)</signature>
        <signature>.*KDevelop::TypeRepository::increaseReferenceCount\(unsigned int, KDevelop::ReferenceCountManager\*\)</signature>
        <signature>.*KDevelop::TypeRepository::decreaseReferenceCount\(unsigned int, KDevelop::ReferenceCountManager\*\)</signature>
        <signature>.*KDevelop::typeRepositoryManager\(\)</signature>
        <signature>.*KDevelop::operator==\(const KDevelop::BuildItem&amp;, const KDevelop::BuildItem&amp;\)</signature>
        <signature>.*KDevelop::temporaryHashUnsureTypeDatam_types\(\)</signature>
    </exclude>
    <functions>
        <!-- everything in that namespace.. -->
        <name>KDevelop::.*</name>
        <name>Sublime::.*</name>
    </functions>
