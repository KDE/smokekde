<config>
    <moduleName>soprano</moduleName>
    <parentModules>
        <module>qt</module>
    </parentModules>
    <parts>10</parts>
    <scalarTypes>
        <!-- QString is a class, but represented as a scalar (#) in munged names -->
        <typeName>QString</typeName>
    </scalarTypes>
    <voidpTypes>
        <!-- both are classes, but they are represented as Smoke::t_voidp -->
        <typeName>QStringList</typeName>
        <typeName>QString</typeName>
    </voidpTypes>
    <exclude>
        <!-- exclude private stuff -->
        <signature>.*Soprano::.*::d</signature>
    </exclude>
    <functions>
        <name>^Soprano::.*</name>
    </functions>
    <classList>
        <class>Soprano</class>
        <class>Soprano::Backend</class>
        <class>Soprano::BackendSetting</class>
        <class>Soprano::BindingSet</class>
        <class>Soprano::Client</class>
        <class>Soprano::Client::DBusClient</class>
        <class>Soprano::Client::DBusModel</class>
        <class>Soprano::Client::DBusNodeIterator</class>
        <class>Soprano::Client::DBusQueryResultIterator</class>
        <class>Soprano::Client::DBusStatementIterator</class>
        <class>Soprano::Client::LocalSocketClient</class>
        <class>Soprano::Client::SparqlModel</class>
        <class>Soprano::Client::TcpClient</class>
        <class>Soprano::Error</class>
        <class>Soprano::Error::Error</class>
        <class>Soprano::Error::ErrorCache</class>
        <class>Soprano::Error::Locator</class>
        <class>Soprano::Error::ParserError</class>
        <class>Soprano::FilterModel</class>
        <class>Soprano::Graph</class>
        <class>Soprano::Index</class>
        <class>Soprano::Index::CLuceneIndex</class>
        <class>Soprano::Index::IndexFilterModel</class>
        <class>Soprano::Index::QueryHit</class>
        <class>Soprano::Inference</class>
        <class>Soprano::Inference::InferenceModel</class>
        <class>Soprano::Inference::NodePattern</class>
        <class>Soprano::Inference::Rule</class>
        <class>Soprano::Inference::RuleParser</class>
        <class>Soprano::Inference::RuleSet</class>
        <class>Soprano::Inference::StatementPattern</class>
        <class>Soprano::Iterator</class>
        <class>Soprano::IteratorBackend</class>
        <class>Soprano::LanguageTag</class>
        <class>Soprano::LiteralValue</class>
        <class>Soprano::Model</class>
        <class>Soprano::Node</class>
        <class>Soprano::NodeIterator</class>
        <class>Soprano::Parser</class>
        <class>Soprano::Plugin</class>
        <class>Soprano::PluginManager</class>
        <class>Soprano::Query</class>
        <class>Soprano::QueryResultIterator</class>
        <class>Soprano::RdfSchemaModel</class>
        <class>Soprano::Serializer</class>
        <class>Soprano::Server</class>
        <class>Soprano::Server::DBusExportIterator</class>
        <class>Soprano::Server::DBusExportModel</class>
        <class>Soprano::Server::ServerCore</class>
        <class>Soprano::Statement</class>
        <class>Soprano::StatementIterator</class>
        <class>Soprano::StorageModel</class>
        <class>Soprano::Util</class>
        <class>Soprano::Util::AsyncModel</class>
        <class>Soprano::Util::AsyncResult</class>
        <class>Soprano::Util::MutexModel</class>
        <class>Soprano::Util::ReadOnlyModel</class>
        <class>Soprano::Util::SignalCacheModel</class>
        <class>Soprano::Util::SimpleNodeIterator</class>
        <class>Soprano::Util::SimpleStatementIterator</class>
        <class>Soprano::Vocabulary</class>
        <class>Soprano::Vocabulary::NAO</class>
        <class>Soprano::Vocabulary::NRL</class>
        <class>Soprano::Vocabulary::OWL</class>
        <class>Soprano::Vocabulary::RDF</class>
        <class>Soprano::Vocabulary::RDFS</class>
        <class>Soprano::Vocabulary::SIL</class>
        <class>Soprano::Vocabulary::Xesam</class>
        <class>Soprano::Vocabulary::XMLSchema</class>
    </classList>
</config>
